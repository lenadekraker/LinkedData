ex:CountryShape a sh:NodeShape ;
    sh:targetClass schema:Country ;
    sh:property [
        sh:path gn:name ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path geo:lat ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:float ;
    ] ;
    sh:property [
        sh:path geo:long ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:float ;
    ] .

